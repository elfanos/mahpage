    "npm-run-all": "^4.1.2",
    "prismjs": "^1.14.0",
    "prop-types": "^15.6.1",
    "radium": "^0.24.0",
    "react": "^16.2.0",
    "react-addons-css-transition-group": "^15.6.2",
    "react-bootstrap": "^0.32.0",
    "react-dom": "^16.2.0",
    "react-prism": "^4.3.2",
    "react-redux": "^5.0.7",
